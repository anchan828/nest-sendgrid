  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
