{
  "name": "@anchan828/nest-sendgrid-packages",
  "version": "0.1.2",
  "description": "SendGrid module for Nest framework (node.js)",
  "homepage": "https://github.com/anchan828/nest-sendgrid#readme",
  "bugs": {
    "url": "https://github.com/anchan828/nest-sendgrid/issues"
