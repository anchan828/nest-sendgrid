    "@lerna-lite/publish": "4.8.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.18.6",
    "eslint": "9.36.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "16.1.6",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "turbo": "2.5.6",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.8.1"
  },
  "packageManager": "npm@11.6.0"
