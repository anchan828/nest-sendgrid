    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "lint-staged": "12.4.2",
    "prettier": "2.6.2",
    "ts-jest": "28.0.3",
